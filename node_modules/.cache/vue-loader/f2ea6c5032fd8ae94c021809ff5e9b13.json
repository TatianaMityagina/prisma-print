ntRect().y;\r\n      // Определение раастояние от начала страницы до элемента\r\n      let start_top = window.scrollY;\r\n      // Вычисление перемещения (скролла) за одну итерацию\r\n      let delta = (top - window.scrollY) / 100;\r\n      // Начало анимации\r\n      for (let i=0;i<100; i++) {\r\n        // Ожидание 1 секунды, деленного на качетсво анимации\r\n        await new Promise(resolve => {\r\n            window.setTimeout(function () {\r\n                resolve()\r\n            }, 1000 / count)\r\n        })\r\n        // Выполнение перемещения на значение delta  у учетом текущего скролла\r\n        window.scrollTo(0, start_top + delta)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped lang=\"scss\">\r\n  .home-print {\r\n    display: flex;\r\n    justify-content: center;\r\n    padding-top: 130px;\r\n    width: 100%;\r\n    margin-bottom: 190px;\r\n  }\r\n\r\n  .home-print__container {\r\n    display: flex;\r\n    flex-direction: column;\r\n    padding: 0 20px;\r\n    max-width: 1720px;\r\n    width: 100%;\r\n  }\r\n\r\n  .home-print__container h2 {\r\n    margin: 0 0 70px 0;\r\n\r\n    font-weight: 600;\r\n    font-size: 68px;\r\n    line-height: 100%;\r\n    color: #961E50;\r\n  }\r\n\r\n  .home-print__list {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    width: 100%;\r\n  }\r\n\r\n  .home-print__item {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    max-width: 317px;\r\n  }\r\n\r\n  .home-print__image-wrapper {\r\n    position: relative;\r\n    margin-bottom: 10px;\r\n\r\n    &:hover {\r\n      .home-print__btn {\r\n        display: flex;\r\n        transition-delay: 0.4s;\r\n      }\r\n\r\n      img {\r\n        opacity: 0.8;\r\n      }\r\n    }\r\n  }\r\n\r\n  .home-print__btn {\r\n    position: absolute;\r\n    top: 45%;\r\n    left: calc(50% - 55px);\r\n\r\n    display: none;\r\n    align-items: cent